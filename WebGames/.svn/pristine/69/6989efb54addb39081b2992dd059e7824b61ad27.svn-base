package edu.usc.cct.rapport.web_games.client.trading_area;

import com.google.gwt.place.shared.PlaceTokenizer;
import com.google.gwt.place.shared.Prefix;

import edu.usc.cct.rapport.web_games.client.WebGamesPlaceTokenizer;

@Prefix(TradingAreaPlace.placeName)
public class TradingAreaPlaceTokenizer extends WebGamesPlaceTokenizer implements PlaceTokenizer<TradingAreaPlace> {

	@Override
	public String getToken(final TradingAreaPlace place) {
		return super.getToken(place);
	};


	@Override
	public TradingAreaPlace getPlace(final String token) {
		final TradingAreaPlace result = placeFactory.createTradingAreaPlace(token);
		return result;
	};

};
